{
  "@context": {
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "owl": "http://www.w3.org/2002/07/owl#",
    "brapi": "https://brapi.org/rdf/",

    "Germplasm": "brapi:Germplasm#",
    "Donor": "Germplasm:Donor#",
    "Attribute": "Germplasm:Attribute#",

    "ObservationUnit": "brapi:ObservationUnit#",
    "Observation": "brapi:Observation#",
    "Treatment": "brapi:Treatment#",
    "ObservationUnitXref": "brapi:ObservationUnitXref#",
    "ObservationVariable": "brapi:ObservationVariable#",

    "Program": "brapi:Program#",
    "Trial": "brapi:Trial#",
    "Study": "brapi:Study#",
    "Location": "brapi:Location#",

    "properties": {
      "@reverse": "rdfs:domain",
      "@type": "@id"
    },
    "range": {
      "@id": "rdfs:range",
      "@type": "@id"
    }
  },
  "@graph": [{
    "@id": "Germplasm:",
    "@type": "rdfs:Class",
    "properties": [
      "Germplasm:germplasmDbId",
      "Germplasm:accessionNumber",
      "Germplasm:genus",
      "Germplasm:species",
      "Germplasm:speciesAuthority",
      "Germplasm:subtaxa",
      "Germplasm:subtaxaAuthority",
      "Germplasm:countryOfOriginCode",
      "Germplasm:instituteCode",
      "Germplasm:biologicalStatusOfAccessionCode",
      "Germplasm:collectingOrAcquisitionSourceCode",
      "Germplasm:typeOfGermplasmStorageCode",
      "Germplasm:acquisitionDate",
      {
        "@id": "Germplasm:donors",
        "range": "Donor:"
      },
      {
        "@id": "Germplasm:taxons",
        "range": "Taxon:"
      }
    ]
  }, {
    "@id": "Donor:",
    "@type": "rdfs:Class",
    "properties": [
      "Donor:donorAccessionNumber",
      "Donor:donorInstituteCode",
      "Donor:donorInstituteName",
      "Donor:donorGermplasmPUI"
    ]
  }, {
    "@id": "Attribute:",
    "@type": "rdfs:Class",
    "properties": [
      "Attribute:attributeDbId",
      "Attribute:attributeName",
      "Attribute:attributeCode",
      "Attribute:value",
      "Attribute:determinedDate"
    ]
  }, {
    "@id": "ObservationUnit:",
    "@type": "rdfs:Class",
    "properties": [
      "ObservationUnit:observationUnitDbId",
      "ObservationUnit:observationUnitName",
      "ObservationUnit:observationLevel",
      "ObservationUnit:plotNumber",
      "ObservationUnit:plantNumber",
      "ObservationUnit:blockNumber",
      "ObservationUnit:replicate",
      "ObservationUnit:entryType",
      "ObservationUnit:entryNumber",
      "ObservationUnit:X",
      "ObservationUnit:Y",
      {
        "@id": "ObservationUnit:observationUnitXrefs",
        "range": "ObservationUnitXref:"
      },
      {
        "@id": "ObservationUnit:germplasm",
        "range": "Germplasm:"
      },
      {
        "@id": "ObservationUnit:study",
        "range": "Study:"
      },
      {
        "@id": "ObservationUnit:treatments",
        "range": "Treatment:"
      },
      {
        "@id": "ObservationUnit:observations",
        "range": "Observation:"
      }
    ]
  }, {
    "@id": "Treatment:",
    "@type": "rdfs:Class",
    "properties": [
      "Treatment:factor",
      "Treatment:modality"
    ]
  }, {
    "@id": "Observation:",
    "@type": "rdfs:Class",
    "properties": [
      "Observation:observationDbId",
      "Observation:season",
      "Observation:collector",
      "Observation:value",
      "Observation:observationTimeStamp",
      {
        "@id": "Observation:variable",
        "range": "ObservationVariable:"
      }
    ]
  }, {
    "@id": "Program:",
    "@type": "rdfs:Class",
    "properties": [
      "Program:programDbId",
      "Program:name",
      "Program:abbreviation",
      "Program:objective",
      "Program:leadPerson"
    ]
  }, {
    "@id": "Study:",
    "@type": "rdfs:Class",
    "properties": [
      "Study:studyDbId",
      "Study:studyName",
      "Study:studyDescription",
      "Study:studyType",
      "Study:seasons",
      "Study:license",
      "Study:startDate",
      "Study:endDate",
      "Study:active",
      {
        "@id": "Study:trial",
        "range": "Trial:"
      },
      {
        "@id": "Study:location",
        "range": "Location:"
      },
      {
        "@id": "Study:program",
        "range": "Program:"
      }
    ]
  }, {
    "@id": "Trial:",
    "@type": "rdfs:Class",
    "properties": [
      "Trial:trialDbId",
      "Trial:trialName",
      "Trial:startDate",
      "Trial:endDate",
      "Trial:active",
      {
        "@id": "Trial:program",
        "range": "Program:"
      },
      {
        "@id": "Trial:study",
        "range": "Study:"
      }
    ]
  }, {
    "@id": "Location:",
    "@type": "rdfs:Class",
    "properties": [
      "Location:locationDbId",
      "Location:name",
      "Location:abreviation",
      "Location:locationType",
      "Location:countryCode",
      "Location:countryName",
      "Location:instituteName",
      "Location:instituteAdress",
      "Location:latitude",
      "Location:longitude",
      "Location:altitude"
    ]
  }]
}
