{
  "@context": {
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "brapi": "https://brapi.org/rdf/",

    "Study": "brapi:Study#",
    "Trial": "brapi:Trial#",
    "Location": "brapi:Location#",

    "studyPUI": "@id",
    "studyDbId": "Study:studyDbId",
    "studyName": "Study:studyName",
    "studyDescription": "Study:studyDescription",
    "studyType": "Study:studyType",
    "seasons": "Study:season",
    "license": "Study:license",
    "startDate": {
      "@id": "Study:startDate",
      "@type": "xsd:date"
    },
    "endDate": {
      "@id": "Study:endDate",
      "@type": "xsd:date"
    },
    "active": {
      "@id": "Study:active",
      "@type": "xsd:boolean"
    },

    "trial": "Trial:trial",
    "trialPUI": {
      "@id": "Study:trial",
      "@type": "@id"
    },

    "location": "Study:location",
    "locationPUI": {
      "@id": "Study:location",
      "@type": "@id"
    }
  }
}
